<full-course-offering
    xmlns="https://source.jasig.org/schemas/portlet/courses/course-catalog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:courses="https://source.jasig.org/schemas/portlet/courses/common"
    xsi:schemaLocation="https://source.jasig.org/schemas/portlet/courses/course-catalog ../../../../../../courses-portlet-api/src/main/resources/xsd/generic/course-catalog.xsd"
    code="BUS256" title="Statistical Analysis" credits="5">

    <description>
        Statistical techniques to be used in guiding business decisions. Introduction to descriptive techniques, probability, estimation, hypothesis testing, regression, correlation and related concepts.
    </description>

    <prerequisites>
        Prerequisite: MATH 095 with a minimum grade of C- or assessment above MATH 095
    </prerequisites>

    <course-section code="136HOL">
        <course-meeting>
            <courses:location>
                <courses:displayName>SSS 114</courses:displayName>
                <courses:identifier>SSS</courses:identifier>
            </courses:location>
            <courses:time>MW 4:00-5:20P</courses:time>
        </course-meeting>
        <instructor>
            <courses:abbreviation>Faculty</courses:abbreviation>
            <courses:fullName>Joe Faculty</courses:fullName>
            <courses:identifier>faculty</courses:identifier>
        </instructor>
    </course-section>

</full-course-offering>